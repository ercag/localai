{
  "name": "localai",
  "displayName": "LocalAI",
  "description": "Local AI coding assistant powered by Ollama",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Machine Learning",
    "Chat"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "localai.openChat",
        "title": "LocalAI: Open Chat",
        "icon": "$(hubot)"
      },
      {
        "command": "localai.applyEdit",
        "title": "Apply Changes",
        "icon": "$(check)",
        "enablement": "localai.hasPendingEdit"
      },
      {
        "command": "localai.rejectEdit",
        "title": "Reject Changes",
        "icon": "$(close)",
        "enablement": "localai.hasPendingEdit"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "localai.openChat",
          "group": "navigation@99"
        },
        {
          "command": "localai.applyEdit",
          "when": "localai.hasPendingEdit",
          "group": "navigation@1"
        },
        {
          "command": "localai.rejectEdit",
          "when": "localai.hasPendingEdit",
          "group": "navigation@2"
        }
      ]
    },
    "configuration": {
      "title": "LocalAI",
      "properties": {
        "localai.ollamaUrl": {
          "type": "string",
          "default": "https://antonio-significant-himself-phillips.trycloudflare.com",
          "description": "Ollama API URL"
        },
        "localai.vllmUrl": {
          "type": "string",
          "default": "http://localhost:8000",
          "description": "vLLM API URL (OpenAI-compatible endpoint)"
        },
        "localai.defaultModel": {
          "type": "string",
          "default": "llama3.2",
          "description": "Default model to use for chat"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "ts-loader": "^9.5.4",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
